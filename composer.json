{
    "name": "dzhdmitry/tinkoff-invest-api",
    "description": "PHP client for Tinkoff Invest API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Dzhuleba",
            "email": "dzhdmitry@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0",
        "symfony/serializer": "^5.1",
        "symfony/property-access": "^5.1",
        "psr/log": "^1.1",
        "amphp/websocket-client": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Dzhdmitry\\TinkoffInvestApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dzhdmitry\\TinkoffInvestApi\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.9"
    }
}
